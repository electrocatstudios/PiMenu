syntax = "proto3";

package proto;

message Timeout {
    int64 length = 1;
    string returnScreen = 2;
    int64 showCountdown = 3;
}

message Request {
  string line1 = 1;
  string line2 = 2;
  string line3 = 3;
  string line4 = 4;
  string line5 = 5;
  string name = 6;
  Timeout timeout=7;

}

message Response {
  string status = 1;
}

service AddService {
    rpc NewScreen(Request) returns (Response);
}